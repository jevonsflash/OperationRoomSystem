    <SolidColorBrush x:Key="TileBrush4">#576fcc</SolidColorBrush>
    <SolidColorBrush x:Key="TileBrush5">#0076d6</SolidColorBrush>

    <SolidColorBrush x:Key="IconBrush1">#d6ac41</SolidColorBrush>
    <SolidColorBrush x:Key="IconBrush2">#3c7acc</SolidColorBrush>
    <SolidColorBrush x:Key="IconBrush3">#36cf5e</SolidColorBrush>

    <SolidColorBrush x:Key="ApplicationAccentBrush">Red</SolidColorBrush>
    <ImageBrush ImageSource="../Icon/arrow_up.png"
                x:Key="IconBrush4"></ImageBrush>
    <ImageBrush ImageSource="../Icon/arrow_down.png"
                x:Key="IconBrush5"></ImageBrush>
    <ImageBrush x:Key="ApplicationBackgroundBrush"
                ImageSource="../Assets/main_background.png"></ImageBrush>
